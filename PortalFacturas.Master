<html xmlns="http://www.w3.org/1999/xhtml" >
<head id="Head1" runat="server">
    <title>Portal de facturas</title>
    <%--<meta http-equiv="X-UA-Compatible" content="IE=6" />--%>
    <link href="App_Themes/PortalCSS.css" type="text/css" rel="stylesheet"/> 
    <script type='text/JavaScript' src='scripts\FuncValidaciones.js'></script>
    <script type='text/JavaScript' src='scripts\scw.js'></script>
    <style type="text/css">
        #marMensaje
        {
            height: 197px;
        }
    </style>
</head>
<body leftmargin="0" topmargin="0"  >
    <form id="form2" runat="server"  enctype="multipart/form-data">
    <asp:ScriptManager ID="ScriptManager1" runat="server" enablepagemethods="true" />
    <div class="header">
        <div class="logoEmp"><img src="images/logoLands-icono"/> <p> PORTAL <br /> <b>FACTURAS</b></p></div>
        <div class="logoPF"><img src="images/LogoPF.png" width="170" height="50" runat="server" id="imgLogo"/></div>
        <div class="logoSSC"><img src="images/SSC.png" width="48" height="57"/></div>
        <div class="logout"><asp:ImageButton ID="btnSalir" runat="server" 
                            ImageUrl="~/Images/btnSalir.png" CommandName="Salir"  OnClientClick="window.close();" OnClient="btnlogout_Click"/>
                        <input type="submit" id="iframe" class="submit" name="go" value="submit" style="position: absolute; left: -2000px"/>
        </div>
    </div>
    <div>
       <div class="menu-frame" >
            <div id="cssmenu">
	            <ul id="lista" runat="server">
	            </ul>
            </div>
            <div>
                <br />
                <br />
                <font color="#F07D00" face="Helvetica, Arial, sans-serif" size="2">
                <marquee id="marMensaje" runat="server" direction="up" width="150"  scrolldelay="300"
                    color="#64350b" >Mensaje</marquee>
                </font>
            </div>
       </div> 
        <div id="Div2" class="content-frame">
            <asp:ContentPlaceHolder ID="MainPlaceHolder" runat="server" >
            </asp:ContentPlaceHolder>
        </div>
     </div>
     </form>
